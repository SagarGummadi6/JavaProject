public class Main{
    //static variable
    static  String school_name = "River High School";

    //instance variable
    public int age=28;

    public void call(){
        //local variable
        String Name = "karl";
        System.out.println("school name: " + Main.school_name);
        System.out.println("Name: " + Name);
        System.out.println("Age: " + age);

    }

    public static void main(String[] args){

        Main obj = new Main();
        obj.call();
    }


}
