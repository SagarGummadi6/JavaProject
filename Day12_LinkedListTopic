import java.util.LinkedList;

public class LinkedListTopic {
    public static void main(String[] args){
        LinkedList<Integer> list = new LinkedList<>();
        list.add(10);
        list.add(20);
        list.add(30);

        System.out.println(list);
        list.add(1, 15);
        System.out.println(list);
        list.removeLast();
        System.out.println(list);
    }
}
