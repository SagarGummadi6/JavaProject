public class PrimeNumber {
    public static void main(String[] args){
        int start = 2;
        int end = 30;
        int i, j;
        for(i=start;i<=end;i++){
            for(j=2;j<=i;j++){
                if(i%j==0){
                    break;
                }
            }
            if(i==j){
                System.out.println(j + " ");
            }
        }
    }
}
